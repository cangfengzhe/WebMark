{
  "author":"Pidong Li",
  "email":"hope-dream@163.com",
    "name": "WebMark",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "48": "icon.png"
    },
    
    "permissions": [
    "tabs", "http://*/*", // 允许扩展使用chrome.tabs或者chrome.windows模块,
    "storage"

  ],
   
   "content_scripts": [ {
      "js": [ "myscript.js"],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "browser_action": {
      "name": "设置颜色和快捷键",
      "default_icon": "icon.png",
       "default_popup": "popup.html"
  }
  
}